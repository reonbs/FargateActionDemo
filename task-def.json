{
    "family": "sample-fargate2", 
    "executionRoleArn": "arn:aws:iam::556096966973:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "fargate-app2", 
            "image": "httpd:2.4", 
            "portMappings": [
                {
                    "containerPort": 81, 
                    "hostPort": 81, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}


