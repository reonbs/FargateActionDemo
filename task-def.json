{
    "family": "sample-fargate3", 
    "executionRoleArn": "arn:aws:iam::556096966973:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "fargate-app3", 
            "image": "testfargate", 
            "portMappings": [
                {
                    "containerPort": 82, 
                    "hostPort": 82, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}


